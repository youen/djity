#!/usr/bin/env python

from setuptools import setup,find_packages

import {package_name}

setup(name={package_name}.pip_name,
    version={package_name}.version,
    description={package_name}.description,
    author={package_name}.author,
    author_email={package_name}.author_email,
    url={package_name}.url,
    packages=['{package_name}'],
    package_data={{'{package_name}':[
		'templates/{package_name}/*.html',
		'media/*/*']
		}},
)
